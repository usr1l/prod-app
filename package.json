{
  "name": "prod-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev": "FLASK_DEBUG=1 pip3 install -r requirements.txt && python3 -m flask --app api/index run -p 8000 --reload",
    "flask-local-dev": "FLASK_DEBUG=1 python3 -m flask --app api/index run -p 8000 --reload",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run flask-dev\"",
    "local-dev": "concurrently \"npm run next-dev\" \"npm run flask-local-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.18",
    "@reduxjs/toolkit": "^2.2.0",
    "@vercel/python": "^4.1.1",
    "axios": "^1.6.5",
    "concurrently": "^8.2.2",
    "framer-motion": "^11.0.3",
    "next": "14.1.0",
    "react": "^18",
    "react-card-flip": "^1.2.2",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "^5.0.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "redux-logger": "^3.0.6",
    "tailwindcss": "^3.3.0"
  }
}
